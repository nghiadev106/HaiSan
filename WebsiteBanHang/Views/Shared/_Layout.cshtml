    <script type="text/javascript">
        // hide #back-top first
        $("#back-top").hide();

        // fade in #back-top
        $(window).scroll(function () {
            if ($(this).scrollTop() > 100) { $('#back-top').fadeIn(); }
            else { $('#back-top').fadeOut(); }
        });

        // scroll body to 0px on click
        $('#back-top').click(function () {
            $('html,body').animate({ scrollTop: 0 }, 400);
            return false;
        });
    </script>

